// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")


}

generator client {
  provider = "prisma-client-js"
}

model Car {
  id        Int        @id @default(autoincrement())
  make      String
  model     String
  year      Int
  bookings  Booking[]  // Add this line to establish reverse relation with Booking
}

model Booking {
  id        Int      @id @default(autoincrement())
  carId     Int
  startDate DateTime
  endDate   DateTime
  car       Car      @relation(fields: [carId], references: [id])
}
