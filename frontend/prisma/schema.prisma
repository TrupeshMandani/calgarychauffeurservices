// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")


}

generator client {
  provider = "prisma-client-js"
}

model Car {
  id    Int     @id @default(autoincrement())
  name  String? // Make name optional
  type  String? // Make type optional
  price String? // Make price optional
  img   String? // Make img optional
  url   String? // Add url column
  bookings Booking[]
}


model Booking {
  id        Int      @id @default(autoincrement())
  startDate DateTime
  endDate   DateTime
  carId     Int
  car       Car      @relation(fields: [carId], references: [id]) // Define the relation from Booking to Car
}

